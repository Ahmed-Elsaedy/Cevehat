<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@Page.Title</title>

    <!-- Bootstrap -->
    <link href="~/Template/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Bootstrap -->
    <!-- Main Style -->
    <link href="~/Template/css/style.css" rel="stylesheet" />
    <!-- Main Style -->
    <!-- fonts -->
    <link href='http://fonts.googleapis.com/css?family=Nunito:300,400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
    <!-- fonts -->

    @RenderSection("head", required: false)
</head>

<body>
    <div id="wrapper">
        <!-- start main wrapper -->
        <div id="header">
            <!-- start main header -->
            <div class="top-line">&nbsp;</div>
            @Html.Partial("_JB_LoginPartial")
            
            <!-- top -->
            <div class="container">
                <!-- container -->
                <div class="row">
                    <div class="col-md-4">
                        <!-- logo -->
                        <a href="/Default/Index" title="Job Board" rel="home">
                            <img class="main-logo" src="~/Template/images/cevehatLogo.png" alt="job board" />
                        </a>
                    </div>
                    <!-- logo -->
                    <div class="col-md-8 main-nav float-right" style="float: right;">
                        <!-- Main Navigation -->
                        <a id="touch-menu" class="mobile-menu" href="#">
                            <i class="fa fa-bars fa-2x"></i>
                        </a>
                        <nav>
                            <ul class="menu" style="float: right;">
                                <li>
                                    @Html.ActionLink("HOME", "Index", "Default")
                                </li>
                                <li>
                               @Html.ActionLink("JOB SEARCH", "Index", "AutoCompletee")
                     @*               <a href="#">JOB SEARCH</a>*@
                                </li>
                                <li>
                                    @Html.ActionLink("Add Job", "AddJob", "CompanyProfile")
                                </li>
                                <li>
                                    <a href="post-resume.html">POST A RESUME</a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!-- Main Navigation -->
                    <div class="clearfix"></div>
                </div>
            </div>
            <!-- container -->
        </div>
        <!-- end main header -->

        @RenderBody()

        <div id="footer">
