            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Skill_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Skill_Id }) |
